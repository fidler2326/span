<div class="page-title">
  <h1>All Time Entries</h1>
</div>

<table class="table table-striped">
  <tr>
    <th>Project</th>
    <th>Hours</th>
    <th>Actions</th>
  </tr>
  <% @time_entries.each do |time| %>
    <tr>
      <td><%= time.project.name %></td>
      <td><%= time.hours %></td>
      <td><%= link_to 'Edit', edit_time_entry_path(time) %></td>
    </tr>
  <% end %>
</table>
